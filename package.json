{
  "name": "crow-agent",
  "version": "0.2.1",
  "description": "Automatically add dark/light mode functionality to Tailwind CSS projects through natural language commands",
  "main": "dist/index.js",
  "bin": {
    "crow": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && cp -r src/templates dist/",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "test": "jest",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "tailwindcss",
    "dark-mode",
    "cli",
    "automation",
    "react",
    "vue",
    "typescript"
  ],
  "author": "Alex McKee",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@types/inquirer": "^9.0.9",
    "@vue/compiler-sfc": "^3.3.0",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "cli-spinners": "^3.2.0",
    "commander": "^11.0.0",
    "fast-glob": "^3.3.0",
    "inquirer": "^12.9.4",
    "jscodeshift": "^0.15.0",
    "ora": "^7.0.0",
    "simple-git": "^3.20.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.0",
    "@types/jest": "^29.5.0",
    "@types/jscodeshift": "^0.11.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexmckee/crow-agent.git"
  },
  "bugs": {
    "url": "https://github.com/alexmckee/crow-agent/issues"
  },
  "homepage": "https://github.com/alexmckee/crow-agent#readme"
}
